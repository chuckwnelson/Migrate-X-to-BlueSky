{
  "compilerOptions": {
      "target": "ES2020",
      "module": "ES2020",
      "strict": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "baseUrl": "./src",
      "paths": {
        "@/*": ["*"]
      }
    },
    "exclude": ["node_modules"]
}
